{
  "name": "react-template-router",
  "version": "0.3.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "run-p build:*",
    "build:dev": "cross-env HOST_NAME=dev react-app-rewired build",
    "build:test": "cross-env HOST_NAME=test react-app-rewired build",
    "build:pro": "cross-env HOST_NAME=pro react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "dll":
      "rimraf public/mainfest && rimraf public/libs && webpack --config config-overrides/webpack.dll.js"
  },
  "lint-staged": {
    "src/**/*.js": ["lint", "git add"]
  }
}
